{
  "name": "exercise-joins",
  "version": "1.0.0",
  "description": "A fun challenge to find unclaimed luggage",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "test": "vitest",
    "dev": "run-p dev:*",
    "dev:server": "nodemon server/index.js",
    "dev:css": "tailwindcss -i ./server/styles/input.css -o ./public/output.css --watch",
    "design": "run-p design:*",
    "design:server": "vite",
    "design:css": "tailwindcss -i ./server/styles/input.css -o ./public/output.css --watch",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "knex",
    "sqlite3",
    "joins"
  ],
  "author": "Dev Academy",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-handlebars": "^7.1.1",
    "knex": "^2.4.2",
    "sqlite3": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.8.0",
    "@faker-js/faker": "^8.0.2",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.8"
  },
  "eslintConfig": {
    "extends": "@devacademy"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
